{
  "name": "ngx-logcat-demo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Demo app for ngx-logcat",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:client-and-server-bundles": "ng build --prod &&  ng run ngx-logcat:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "deploy:firebase": "npm run build:prod && npm run build:doc && firebase deploy",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "lint": "ng lint",
    "test": "ng test",
    "postinstall": "npm run link",
    "link": "npm link ngx-logcat",
    "start": "ng serve",
    "serve:prod": "ng serve --prod",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/platform-server": "^7.2.0",
    "@angular/router": "^7.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@stackblitz/sdk": "^1.2.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.2",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@angularclass/hmr": "~2.1.3",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "express": "^4.16.4",
    "greenkeeper-lockfile": "^1.15.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "protractor": "~5.3.0",
    "reflect-metadata": "~0.1.12",
    "ts-loader": "~4.2.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6",
    "webpack-cli": "^2.0.14",
    "yargs": "^12.0.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  }
}
